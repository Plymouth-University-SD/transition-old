              <option data-scrape="true" value="announcement_type_press-releases">Press releases</option>
              <option data-scrape="true" value="announcement_type_speeches">Speeches</option>
              <option data-scrape="true" value="announcement_type_statements">Statements</option>
            </optgroup>
            <optgroup label="Publications">
              <option data-scrape="false" value="publication_type_consultations">Consultations</option>
              <option data-scrape="false" value="publication_type_corporate-reports">Corporate reports</option>
