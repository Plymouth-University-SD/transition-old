            <option data-scrape="false" data-url="true" value="elsewhere">Exists elsewhere: redirect</option>
            <option data-scrape="false" value="mainstream">Mainstream content</option>
            <option data-scrape="false" value="guidance">Guidance document</option>
            <option data-scrape="false" value="new">New content type</option>
